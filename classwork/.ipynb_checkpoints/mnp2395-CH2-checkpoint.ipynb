    "def binary(num):\n",
    "    if (num == 0):\n",
    "        return 0\n",
    "    res = 0\n",
    "    place = 1\n",
    "    while (num!=0):\n",
    "        res += (num%2)*place\n",
    "        place *= 10\n",
    "        num//=2\n",
    "    return res"
   ]
